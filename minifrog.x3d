<!DOCTYPE html>
<html lang="ru">
<head>  
	<meta charset="utf-8" />
	<link rel="stylesheet" href="https://www.x3dom.org/download/dev/x3dom.css" />
	<script src="https://www.x3dom.org/download/dev/x3dom-full.js"></script>
</head> 
<body>
	<X3D>
		<Scene>
			<Group DEF="Poliwag">
				<Anchor url="http://poke-universe.ru/dex/bw/061.html" parameter="target='_self'"></Anchor>
          <Shape>
            <Appearance>
              <ImageTexture url="рыба.png"></ImageTexture>
            </Appearance>
            <Sphere radius="4"></Sphere>
          </Shape>
        <!--Ноги-->
        <Transform translation="1.4 -4 0">
          <Shape>
            <Appearance>
              <Material diffuseColor='#5792be'></Material>
            </Appearance>
            <Cylinder radius="0.5" height="2"></Cylinder>
          </Shape>
          <Transform translation="0 -0.7 0">
          <Shape>
            <Appearance>
              <Material diffuseColor='#5792be'></Material>
            </Appearance>
            <Sphere radius="0.7"></Sphere>
          </Shape>
        </Transform>
        </Transform>
        <Transform translation="-1.4 -4 0">
          <Shape>
            <Appearance>
              <Material diffuseColor='#5792be'></Material>
            </Appearance>
            <Cylinder radius="0.5" height="2"></Cylinder>
          </Shape>
          <Transform translation="0 -0.7 0">
          <Shape>
            <Appearance>
              <Material diffuseColor='#5792be'></Material>
            </Appearance>
            <Sphere radius="0.7"></Sphere>
          </Shape>
        </Transform>
        </Transform>
      </Group>
		</Scene>
	</X3D>
</body>
</html>